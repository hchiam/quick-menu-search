{
  "name": "Quick Menu Search",
  "version": "1.0.1",
  "description": "Quick Menu Search",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["brain.js"]
    }
  ],
  "browser_action": {
    "default_popup": "structure.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}
